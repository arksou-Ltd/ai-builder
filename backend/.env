# AI Builder API 环境变量配置模板
# 复制此文件为 .env 并填入实际值: cp .env.example .env

# 应用配置
DEBUG=true
ENVIRONMENT=development

# 数据库配置（必须配置）
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=     # <-- 必填：填写你的本地 PostgreSQL 密码
DATABASE_NAME=ai_builder

# CORS 配置
CORS_ORIGINS=["http://localhost:3000"]

# 日志配置
LOG_LEVEL=DEBUG
